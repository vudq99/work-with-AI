#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/module-definition@6.0.1/node_modules/module-definition/bin/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/module-definition@6.0.1/node_modules/module-definition/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/module-definition@6.0.1/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/module-definition@6.0.1/node_modules/module-definition/bin/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/module-definition@6.0.1/node_modules/module-definition/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/module-definition@6.0.1/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../module-definition/bin/cli.js" "$@"
else
  exec node  "$basedir/../module-definition/bin/cli.js" "$@"
fi
