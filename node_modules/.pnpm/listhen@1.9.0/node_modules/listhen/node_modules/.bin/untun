#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/untun@0.1.3/node_modules/untun/bin/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/untun@0.1.3/node_modules/untun/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/untun@0.1.3/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/untun@0.1.3/node_modules/untun/bin/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/untun@0.1.3/node_modules/untun/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/untun@0.1.3/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../untun@0.1.3/node_modules/untun/bin/untun.mjs" "$@"
else
  exec node  "$basedir/../../../../../untun@0.1.3/node_modules/untun/bin/untun.mjs" "$@"
fi
