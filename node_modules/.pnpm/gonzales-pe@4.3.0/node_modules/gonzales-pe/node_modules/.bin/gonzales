#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules/gonzales-pe/bin/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules/gonzales-pe/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules/gonzales-pe/bin/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules/gonzales-pe/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/gonzales-pe@4.3.0/node_modules:/Users/quangvuduong/dev/personal/work-with-AI/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/gonzales.js" "$@"
else
  exec node  "$basedir/../../bin/gonzales.js" "$@"
fi
